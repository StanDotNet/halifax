<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
      <TargetFramework>net5.0</TargetFramework>
      <Description>Halifax Service Foundation Core functionality</Description>
      <summary>Halifax Service Foundation Core functionality</summary>
      <iconUrl>https://raw.githubusercontent.com/andrei-m-code/halifax/main/Icon.png</iconUrl>
      <Authors>andrei-m-code</Authors>
      <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
      <AssemblyVersion>0.0.2.0</AssemblyVersion>
      <FileVersion>0.0.2.0</FileVersion>
      <Version>0.0.2</Version>
      <PackageId>Halifax.Core</PackageId>
      <PackageProjectUrl>https://github.com/andrei-m-code/halifax</PackageProjectUrl>
      <RepositoryUrl>https://github.com/andrei-m-code/halifax</RepositoryUrl>
      <PackageLicenseExpression>MIT</PackageLicenseExpression>
      <PackageDescription>Core Service Foundation library</PackageDescription>
      <PackageTags>api service-foundation services microservices correlation-id base</PackageTags>
      <RepositoryType>git</RepositoryType>
      <PackageReleaseNotes>
v0.0.2
v0.0.1
- Initial release
      </PackageReleaseNotes>
    </PropertyGroup>


    <ItemGroup>
      <PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="6.12.0" />
    </ItemGroup>

</Project>
